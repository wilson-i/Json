{
  "manifest_version": 3,
  "name": "WeiSi-Json",
  "version": "1.0",
  "description": "格式化JSON数据，使其更易读",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
